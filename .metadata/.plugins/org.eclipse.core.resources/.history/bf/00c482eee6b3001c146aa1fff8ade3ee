package day0404;

class Hello {
	int year, month, day;
	
	Hello(int year, int month, int day){
		this.year = year;
		this.month = month;
		this.day = day;
	}
	
	public void write() {
		System.out.println(year + "년" + month + "월" + day + "일생입니다.");
	}
}

public class Ex11Constructor {
	public static void main(String[] args) {
		
		Hello h1 = new Hello();
		
		Hello h2 = new Hello(1970, 10, 21);
		h2.write();
	}
}
