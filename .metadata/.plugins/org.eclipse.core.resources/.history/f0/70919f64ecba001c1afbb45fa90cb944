package day0413;

public class Ex02_MysqlPerson {

	static final String DRIVER = "com.mysql.cj.jdbc.Driver";
	static final String URL = "jdbc:mysql://localhost:3306/bit901";
//	static final String URL = "jdbc:mysql://localhost:3306/bit901?serverTimezone=Asia/Seoul"; 8버전
	
	public Ex02_MysqlPerson() {
		try {
			Class.forName(DRIVER);
			System.out.println("mysql 드라이버 성공");
		} catch (ClassNotFoundException e) {
			System.out.println("mysql 드라이버 실패: " + e.getMessage());
		}
	}
	
	public static void main(String[] args) {
		Ex02_MysqlPerson person = new Ex02_MysqlPerson();
		
	}
}
